void main(int argc, int argv) {
    int a;
    int b;
    int c;

    Print("Please enter the first number: ");
    a = ReadInteger();
    Print("Please enter the second number: ");
    b = ReadInteger();

    if (b == 0) {
        Print("NO NO NO NO... I can't divide by zero!");
        return;
    }

    c = a / b;

    Print("The result of ", a, " / ", b, " is ", c);
    return;
}